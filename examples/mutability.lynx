mut ma = 1;  -- Mutable binding
a = ma;
ma := 2;
println a  -- 1

ra: Cell Int = cell 1;  -- Mutable cell
mut rb = ra;
rb := cell 2;
println !ra;  -- 1
rc = ra;
rc << 3;
println !ra  -- 3

fn Stash (A: Type) : Type {
    { content: Cell A }
}

fn f @A ({content}: Stash A) : Cell A {
    content
}

stash = { content = cell 0 };
f stash << 1;
assert_eq !stash.content 1
